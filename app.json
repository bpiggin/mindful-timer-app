{
  "expo": {
    "name": "Mindful Timer",
    "slug": "MindfulTimer",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bpiggin.mindfultimer"
    },
    "android": {
      "package": "com.bpiggin.mindfultimer"
    },
    "description": "Simple black and white themed meditation timer."
  }
}
